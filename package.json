{
    "name": "car-tracking",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*",
        "servers/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "^1.2.16"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "npm@8.1.2"
}
